  "private": false,
  "scripts": {
    "eslint-test": "eslint './*.js'",
    "clean": "rm -rf dist",
    "build": "babel src -d dist",
    "prepublish": "yarn clean && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hhru/redux-dev-helper.git"
  },
