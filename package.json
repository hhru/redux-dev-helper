  "author": "Nik Mostovoy (nik.mostovoy@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hhru/redux-dev-helper/issues"
  },
  "homepage": "https://github.com/hhru/redux-dev-helper#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-function-bind": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "@hh.ru/eslint-config": "^8.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5"
  },
  "target": {
