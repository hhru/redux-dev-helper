{
  "name": "@hh.ru/redux-dev-helper",
  "version": "1.1.0",
  "description": "Redux tool which provides a global variable store. Can filter empty fields and subscribe to changes",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "eslint-test": "eslint './*.js'",
    "clean": "rm -rf dist",
    "build": "babel src -d dist",
    "prepublish": "yarn clean && yarn build",
    "release": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hhru/redux-dev-helper.git"
  },
  "keywords": [
    "redux",
    "store",
    "store-subscribe"
  ],
  "author": "Nik Mostovoy (nik.mostovoy@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hhru/redux-dev-helper/issues"
  },
  "homepage": "https://github.com/hhru/redux-dev-helper#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/plugin-proposal-function-bind": "^7.10.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@hh.ru/eslint-config": "^9.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1"
  },
  "target": {
    "main": {
      "isLibrary": true
    }
  },
  "files": [
    "dist"
  ]
}
